{
  "expo": {
    "name": "EcoChargeHub",
    "slug": "EcoChargeHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/green-energy-app-icon.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/green-energy-splash.gif",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/constants/*",
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/green-energy-splash.gif",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cb7c95fa-115f-4afb-95d8-e53be04037cf"
      }
    }
  }
}
